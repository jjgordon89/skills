{
  "sources": [
    {
      "name": "arxiv",
      "enabled": true,
      "description": "arXiv每日论文速递 - 专注于大语言模型",
      "categories": [
        "cs.CL",
        "cs.AI",
        "cs.LG"
      ],
      "max_results": 5,
      "update_interval": "daily"
    },
    {
      "name": "huggingface",
      "enabled": true,
      "description": "HuggingFace每日热门论文",
      "url": "https://huggingface.co/papers",
      "max_results": 5,
      "update_interval": "daily"
    }
  ],
  "output_format": {
    "include_abstract": true,
    "include_authors": true,
    "include_links": true,
    "language": "zh-CN"
  },
  "filter": {
    "keywords": [
      "LLM",
      "Large Language Model",
      "GPT",
      "Transformer",
      "BERT",
      "ChatGPT",
      "agent",
      "reasoning"
    ],
    "exclude_keywords": []
  }
}
