{
  "sources": [
    {
      "name": "arxiv",
      "enabled": true,
      "description": "arXiv每日论文速递",
      "categories": [
        "cs.AI",
        "cs.CL",
        "cs.CV",
        "cs.LG",
        "cs.NE"
      ],
      "max_results": 10,
      "update_interval": "daily"
    },
    {
      "name": "huggingface",
      "enabled": true,
      "description": "HuggingFace每日论文",
      "url": "https://huggingface.co/papers",
      "max_results": 10,
      "update_interval": "daily"
    }
  ],
  "output_format": {
    "include_abstract": true,
    "include_authors": true,
    "include_links": true,
    "language": "zh-CN"
  },
  "filter": {
    "keywords": [],
    "exclude_keywords": []
  }
}
